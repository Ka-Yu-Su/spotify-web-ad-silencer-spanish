{
  "manifest_version": 2,
  "name": "mute-spotify",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["https://open.spotify.com/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "48": "icons/48.png",
    "96": "icons/96.png",
    "128": "icons/128.png",
    "256": "icons/256.png"
  },
  "background": {
    "scripts": ["background.js"]
  }
}
